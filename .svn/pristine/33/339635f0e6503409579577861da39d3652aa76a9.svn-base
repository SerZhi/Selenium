package Step5.Entity;

public class Mail {

	String recipient;
	String subject;
	String text;

	public Mail(String recipient, String subject, String text) {
		this.recipient = recipient;
		this.subject = subject;
		this.text = text;
	}

	public String getRecipient(){
		return recipient;
	}
	
	public String getSubject(){
		return subject;
	}
	
	public String getText(){
		return text;
	}
	
}
